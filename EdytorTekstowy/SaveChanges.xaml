<Window x:Class="EdytorTekstowy.SaveChanges"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:EdytorTekstowy"
        mc:Ignorable="d"
        Title="Plik został zmieniony" 
        Height="150" Width="300">
    <Grid RenderTransformOrigin="0.518,0.298">
        <Grid.RowDefinitions>
            <RowDefinition Height="58*"/>
            <RowDefinition Height="71*"/>
        </Grid.RowDefinitions>
        <TextBlock TextWrapping="WrapWithOverflow" TextAlignment="Center" Grid.Row="0" Text="Zostały wprowadzone zmiany, czy chcesz je zapisać?" VerticalAlignment="Center"></TextBlock>
        <Button Grid.Row="1" Name="Yes" Content="Tak" Margin="10,6,204,7" Click="Yes_Click"/>
        <Button Grid.Row="1" Name="Nie" Content="Nie" Margin="107,6,107,7" Click="Nie_Click"/>
        <Button Grid.Row="1" Name="Anuluj" Content="Anuluj" Margin="204,6,10,7" Click="Anuluj_Click"/>
    </Grid>
</Window>
